#!/usr/bin/ruby
require           'rubygems'
require           'aws-sdk'
require           'whenever-elasticbeanstalk'

# Create the new Whenever::Elasticbeanstalk instance
whenever_eb = Whenever::Elasticbeanstalk.new(Aws::InstanceProfileCredentials.new)
# Remove the cron leader
whenever_eb.remove_cron_leader
